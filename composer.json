{
  "name": "sunnysideup/recently-changed",
  "description": "Work out all the data objects and associated table data that has recently changed",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "dev-tools",
    "dev tool",
    "recently changed",
    "data-analysis",
    "content analysis",
    "migration",
    "make live",
    "copying database",
    "database"
  ],
  "license": "BSD-3-Clause",
  "require": {
    "silverstripe/framework": "^5.0",
    "silverstripe/admin": "^2.0"
  },
  "require-dev": {
    "sunnysideup/sswebpack_engine_only": "dev-master",
    "silverleague/ideannotator": "*",
    "phpunit/phpunit": "^9.6",
    "squizlabs/php_codesniffer": "^3.7",
    "silverstripe/standards": "^1",
    "phpstan/extension-installer": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Sunnysideup\\RecentlyChanged\\": "src/"
    }
  },
  "extra": {
    "expose": [
      "client/dist"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "bin": [
    "bin/npm-build-recently-changed.sh",
    "bin/npm-watch-recently-changed.sh"
  ],
  "authors": [
    {
      "name": "Nicolaas Thiemen Francken",
      "email": "modules@sunnysideup.co.nz",
      "role": "lead developer"
    }
  ]
}
